{
  "name": "tasty-table",
  "version": "0.1.0",
  "description": "A tasty async AngularJS Table directive.",
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "devDependencies": {
    "bower": "^1.3.1",
    "http-server": "^0.6.1",
    "karma": "~0.10",
    "karma-chrome-launcher": "^0.1.4",
    "karma-jasmine": "^0.1.5",
    "karma-ng-html2js-preprocessor": "^0.1.0",
    "karma-spec-reporter": "^0.0.13",
    "protractor": "~0.20.1"
  },
  "scripts": {
    "postinstall": "bower install",

    "start": "http-server -a localhost -p 8000",

    "pretest": "npm install",
    "test": "karma start test/karma.conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run",

    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",

    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zizzamia/tasty-table.git"
  },
  "author": "Leonardo Zizzamia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zizzamia/tasty-table/issues"
  },
  "homepage": "https://github.com/Zizzamia/tasty-table"
}
